@model ContatoModel
@{
    ViewData["Title"] = "Apagar Contato";
}

<div class="text-center">

    <h1 class="display-4">Apagar Contato (@Model.Nome)?</h1>
    <br />
    <a role="button" class="btn btn-danger" asp-controller="Contato" asp-route-id="@Model.Id" asp-action="Apagar">Confirmar</a>
    <a role="button" class="btn btn-outline-primary" asp-controller="Contato" asp-action="Index">Cancelar</a>

</div>